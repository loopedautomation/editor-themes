{
  "$schema": "vscode://schemas/color-theme",
  "name": "Looped Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {},
  "tokenColors": [
    {
      "scope": [
        "filename.entity.name.filename.find-in-files"
      ],
      "settings": {
        "foreground": "#FCFD28"
      }
    },
    {
      "scope": [
        "keyword.keyword",
        "keyword.support.type.property-name.json"
      ],
      "settings": {
        "foreground": "primary_hover"
      }
    },
    {
      "scope": [
        "line_number.constant.numeric.line-number.find-in-files - match"
      ],
      "settings": {
        "foreground": "#ec4899A0"
      }
    },
    {
      "scope": [
        "strikethrough.markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": [
        "token.debug-token"
      ],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "scope": [
        "token.error-token"
      ],
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": [
        "token.info-token"
      ],
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": [
        "token.warn-token"
      ],
      "settings": {
        "foreground": "#cca700"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "builtin_constant.constant.language",
        "builtin_constant.keyword.operator.ternary.jsx",
        "builtin_constant.meta.brace.round.jsx",
        "builtin_constant.meta.brace.square.jsx",
        "builtin_constant.punctuation.definition.block.jsx",
        "builtin_constant.punctuation.definition.binding-pattern.object.jsx",
        "builtin_constant.punctuation.definition.template-expression.begin.jsx",
        "builtin_constant.punctuation.definition.template-expression.end.jsx",
        "builtin_constant.keyword.operator.ternary.js",
        "builtin_constant.meta.brace.round.js",
        "builtin_constant.meta.brace.square.js",
        "builtin_constant.punctuation.definition.block.js",
        "builtin_constant.punctuation.definition.binding-pattern.object.js",
        "builtin_constant.punctuation.definition.template-expression.begin.js",
        "builtin_constant.punctuation.definition.template-expression.end.js",
        "builtin_constant.keyword.operator.ternary.ts",
        "builtin_constant.meta.brace.round.ts",
        "builtin_constant.meta.brace.square.ts",
        "builtin_constant.punctuation.definition.block.ts",
        "builtin_constant.punctuation.definition.binding-pattern.object.ts",
        "builtin_constant.punctuation.definition.template-expression.begin.ts",
        "builtin_constant.punctuation.definition.template-expression.end.ts",
        "builtin_constant.keyword.operator.ternary.tsx",
        "builtin_constant.meta.brace.round.tsx",
        "builtin_constant.meta.brace.square.tsx",
        "builtin_constant.punctuation.definition.block.tsx",
        "builtin_constant.punctuation.definition.template-expression.begin.tsx",
        "builtin_constant.punctuation.definition.template-expression.end.tsx",
        "builtin_constant.punctuation.section.embedded.begin.js",
        "builtin_constant.punctuation.section.embedded.end.js",
        "builtin_constant.punctuation.section.embedded.begin.jsx",
        "builtin_constant.punctuation.section.embedded.end.jsx",
        "builtin_constant.punctuation.section.embedded.begin.ts",
        "builtin_constant.punctuation.section.embedded.end.ts",
        "builtin_constant.punctuation.section.embedded.begin.tsx",
        "builtin_constant.punctuation.section.embedded.end.tsx"
      ],
      "settings": {
        "foreground": "error"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "class.entity.name.type",
        "class.entity.name.class",
        "class.entity.name.namespace",
        "class.entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#ECEEF5"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment.comment",
        "comment.definition.comment"
      ],
      "settings": {
        "foreground": "comment"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "function.entity.name.function",
        "function.meta.function-call.python",
        "function.new.expr.js"
      ],
      "settings": {
        "foreground": "string"
      }
    },
    {
      "name": "Inherited class",
      "scope": [
        "inherited_class.entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#e0cbff",
        "fontStyle": "italic underline"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid.invalid"
      ],
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "name": "JSON String",
      "scope": [
        "json_string.meta.structure.dictionary.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#CFCFC2"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "library_class_type.support.type, support.class"
      ],
      "settings": {
        "foreground": "string",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Library constant",
      "scope": [
        "library_constant.support.constant"
      ],
      "settings": {
        "foreground": "primary_hover"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "library_function.support.function"
      ],
      "settings": {
        "foreground": "primary_hover"
      }
    },
    {
      "name": "Library variable",
      "scope": [
        "library_variable.support.other.variable"
      ],
      "settings": {}
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markdown_bold.markup.bold.markdown"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Emphasis",
      "scope": [
        "markdown_emphasis.markup.italic.markdown"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link Title/Description",
      "scope": [
        "markdown_link_title_description.string.other.link.title.markdown,string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "error"
      }
    },
    {
      "name": "Markdown List Punctuation",
      "scope": [
        "markdown_list_punctuation.punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "primary"
      }
    },
    {
      "name": "Markdown Punctuation Definition Link",
      "scope": [
        "markdown_punctuation_definition_link.markup.list.unnumbered.markdown, markup.list.numbered.markdown"
      ],
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markdown_quote.markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#e5e7eb",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Underline Link/Image",
      "scope": [
        "markdown_underline_link_image.markup.underline.link.markdown,markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "foreground"
      }
    },
    {
      "name": "Markup Headings",
      "scope": [
        "markup_headings.markup.heading"
      ],
      "settings": {
        "foreground": "#ec4899"
      }
    },
    {
      "name": "Markup Inline",
      "scope": [
        "markup_inline.markup.inline.raw"
      ],
      "settings": {
        "foreground": "#EC0095"
      }
    },
    {
      "name": "Markup Lists",
      "scope": [
        "markup_lists.markup.list"
      ],
      "settings": {
        "foreground": "#d946ef"
      }
    },
    {
      "name": "Markup Quote",
      "scope": [
        "markup_quote.markup.quote"
      ],
      "settings": {
        "foreground": "#F811A0"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": [
        "markup_setext_header.markup.heading.setext"
      ],
      "settings": {
        "foreground": "#ec4899",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup_styling.markup.bold, markup.italic"
      ],
      "settings": {
        "foreground": "#faf5ff"
      }
    },
    {
      "name": "Number",
      "scope": [
        "number.constant.numeric"
      ],
      "settings": {
        "foreground": "error"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor.optional.js.jsx"
      ],
      "settings": {
        "foreground": "comment"
      }
    },
    {
      "name": "Punctuation Definition",
      "scope": [
        "punctuation.definition.block.js",
        "punctuation.definition.binding-pattern.object.js",
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js"
      ],
      "settings": {
        "foreground": "comment"
      }
    },
    {
      "name": "Punctuation Section",
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.begin.jsx",
        "punctuation.section.embedded.end.jsx"
      ],
      "settings": {
        "foreground": "primary"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": [
        "reset.meta.template.expression"
      ],
      "settings": {
        "foreground": "foreground"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.storage"
      ],
      "settings": {
        "foreground": "foreground"
      }
    },
    {
      "name": "Storage type",
      "scope": [
        "storage_type.storage.type"
      ],
      "settings": {
        "foreground": "primary_hover",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string.string"
      ],
      "settings": {
        "foreground": "string"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "tag_attribute.entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "warning"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "tag_name.entity.name.tag",
        "tag_name.support.class.component.jsx",
        "tag_name.support.class.component.js",
        "tag_name.support.class.component.ts",
        "tag_name.support.class.component.tsx"
      ],
      "settings": {
        "foreground": "#8B8DFF"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "template.punctuation.definition.template-expression",
        "template.punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "primary"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "user_defined_constant.constant.character, constant.other"
      ],
      "settings": {
        "foreground": "foreground"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable.variable"
      ],
      "settings": {
        "foreground": "foreground"
      }
    },
    {
      "name": "diff.changed",
      "scope": [
        "diff_changed.markup.changed"
      ],
      "settings": {
        "foreground": "#8b5cf6"
      }
    },
    {
      "name": "diff.deleted",
      "scope": [
        "diff_deleted.markup.deleted"
      ],
      "settings": {
        "foreground": "#F811A0"
      }
    },
    {
      "name": "diff.header",
      "scope": [
        "diff_header.meta.diff, meta.diff.header"
      ],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "diff.inserted",
      "scope": [
        "diff_inserted.markup.inserted"
      ],
      "settings": {
        "foreground": "#f4a14d"
      }
    }
  ]
}