{
  "$schema": "vscode://schemas/color-theme",
  "name": "Looped Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "debugToolBar.background": "#3d2c5a",
    "editorCursor.foreground": "#faf8ff",
    "editorGroup.border": "#090B0F",
    "editorGroup.dropBackground": "#ffffff10",
    "editorGroupHeader.tabsBackground": "#090B0F",
    "editorHoverWidget.background": "#090B0F",
    "editorHoverWidget.border": "#a397a3",
    "editorIndentGuide.activeBackground1": "#7c5dfa",
    "editorIndentGuide.background1": "#4a4850",
    "editorLineNumber.activeForeground": "#7c5dfa",
    "editorLineNumber.foreground": "#4a4850",
    "editorSuggestWidget.background": "#262438",
    "editorSuggestWidget.border": "#a397a3",
    "editorWhitespace.foreground": "#4a4850",
    "editorWidget.background": "#090B0F",
    "focusBorder": "#7c5dfa",
    "inputOption.activeBorder": "#a397a3",
    "minimap.selectionHighlight": "#ffffff20",
    "peekView.border": "#7c5dfa",
    "peekViewTitle.background": "#090B0F",
    "pickerGroup.foreground": "#79767d",
    "quickInput.background": "#262438",
    "quickInput.foreground": "#faf8ff",
    "quickInputList.focusBackground": "#090B0F",
    "selection.background": "#ffffff20",
    "settings.focusedRowBackground": "#262438",
    "widget.shadow": "#00000080",
    "activityBar.activeBorder": "#7c5dfa",
    "activityBar.background": "#090B0F",
    "activityBar.foreground": "#faf8ff",
    "activityBarBadge.background": "#a855f7",
    "diffEditor.insertedTextBackground": "#48cc2e70",
    "diffEditor.removedTextBackground": "#D02E1F50",
    "dropdown.background": "#090B0F",
    "dropdown.listBackground": "#090B0F",
    "editor.background": "#090B0F",
    "editor.foreground": "#faf8ff",
    "editor.lineHighlightBackground": "#ffffff08",
    "editor.selectionBackground": "#ffffff20",
    "editor.selectionHighlightBackground": "#7c5dfa30",
    "input.background": "#262438",
    "input.foreground": "#faf8ff",
    "inputValidation.errorBackground": "#8b1538",
    "inputValidation.errorBorder": "#a397a3",
    "inputValidation.infoBackground": "#5d4d7a",
    "inputValidation.infoBorder": "#a397a3",
    "inputValidation.warningBackground": "#b87400",
    "inputValidation.warningBorder": "#a397a3",
    "list.activeSelectionBackground": "#262438",
    "list.dropBackground": "#ffffff10",
    "list.errorForeground": "#D02E1F",
    "list.highlightForeground": "#faf8ff",
    "list.hoverBackground": "#262438",
    "list.inactiveSelectionBackground": "#090B0F",
    "list.warningForeground": "#ED9B00",
    "menu.background": "#090B0F",
    "menu.foreground": "#faf8ff",
    "notebook.cellBorderColor": "#090B0F",
    "notebook.cellEditorBackground": "#262438",
    "notebook.cellHoverBackground": "#262438",
    "notebook.cellInsertionIndicator": "#ffffff",
    "notebook.focusedCellBackground": "#262438",
    "notebook.focusedCellBorder": "#7c5dfa",
    "notebook.focusedEditorBorder": "#7c5dfa",
    "panel.background": "#090B0F",
    "panel.border": "#7c5dfa",
    "panelTitle.activeBorder": "#7c5dfa",
    "panelTitle.activeForeground": "#faf8ff",
    "panelTitle.inactiveForeground": "#79767d",
    "peekViewEditor.background": "#262438",
    "peekViewEditor.matchHighlightBackground": "#79767d",
    "peekViewResult.background": "#090B0F",
    "peekViewResult.matchHighlightBackground": "#79767d",
    "peekViewResult.selectionBackground": "#090B0F",
    "sideBar.background": "#090B0F",
    "sideBar.foreground": "#a397a3",
    "statusBarItem.errorBackground": "#D02E1F",
    "statusBarItem.errorForeground": "#faf8ff",
    "statusBarItem.prominentBackground": "#7c5dfa",
    "statusBarItem.prominentForeground": "#faf8ff",
    "statusBarItem.prominentHoverBackground": "#9575ff",
    "statusBarItem.remoteBackground": "#7c5dfa",
    "statusBarItem.remoteForeground": "#faf8ff",
    "statusBarItem.warningBackground": "#ED9B00",
    "statusBarItem.warningForeground": "#faf8ff",
    "tab.activeBorder": "#7c5dfa",
    "tab.activeForeground": "#faf8ff",
    "tab.border": "#090B0F",
    "tab.inactiveBackground": "#090B0F",
    "tab.inactiveForeground": "#a397a3",
    "tab.lastPinnedBorder": "#a397a3",
    "terminal.ansiBlack": "#4a4850",
    "terminal.ansiBlue": "#3b82f6",
    "terminal.ansiBrightBlack": "#79767d",
    "terminal.ansiBrightBlue": "#4d9eff",
    "terminal.ansiBrightCyan": "#19d4ff",
    "terminal.ansiBrightGreen": "#37946A",
    "terminal.ansiBrightMagenta": "#c77dff",
    "terminal.ansiBrightRed": "#ff4d94",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#ED9B00",
    "terminal.ansiCyan": "#0891b2",
    "terminal.ansiGreen": "#37946A",
    "terminal.ansiMagenta": "#a855f7",
    "terminal.ansiRed": "#D02E1F",
    "terminal.ansiWhite": "#faf8ff",
    "terminal.ansiYellow": "#ED9B00",
    "titleBar.activeBackground": "#090B0F",
    "titleBar.activeForeground": "#faf8ff",
    "titleBar.inactiveBackground": "#14161A",
    "titleBar.inactiveForeground": "#79767d"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "definition.comment"
      ],
      "settings": {
        "foreground": "#79767d"
      }
    },
    {
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#faf8ff"
      }
    },
    {
      "scope": [
        "constant.numeric.line-number.find-in-files - match"
      ],
      "settings": {
        "foreground": "#ec4899A0"
      }
    },
    {
      "scope": [
        "keyword",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#9575ff"
      }
    },
    {
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#8b5cf6"
      }
    },
    {
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#F811A0"
      }
    },
    {
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#ec4899"
      }
    },
    {
      "scope": [
        "markup.heading.setext"
      ],
      "settings": {
        "foreground": "#ec4899",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#EC0095"
      }
    },
    {
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#f4a14d"
      }
    },
    {
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#F811A0"
      }
    },
    {
      "scope": [
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#e5e7eb",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#faf8ff"
      }
    },
    {
      "scope": [
        "meta.diff"
      ],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "scope": [
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#D02E1F"
      }
    },
    {
      "scope": [
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#D02E1F"
      }
    },
    {
      "scope": [
        "token.debug-token"
      ],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "scope": [
        "token.error-token"
      ],
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": [
        "token.info-token"
      ],
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": [
        "token.warn-token"
      ],
      "settings": {
        "foreground": "#cca700"
      }
    },
    {
      "name": ".header",
      "scope": [
        "diff"
      ],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "keyword.operator.ternary.jsx",
        "meta.brace.round.jsx",
        "meta.brace.square.jsx",
        "punctuation.definition.block.jsx",
        "punctuation.definition.binding-pattern.object.jsx",
        "punctuation.definition.template-expression.begin.jsx",
        "punctuation.definition.template-expression.end.jsx",
        "keyword.operator.ternary.js",
        "meta.brace.round.js",
        "meta.brace.square.js",
        "punctuation.definition.block.js",
        "punctuation.definition.binding-pattern.object.js",
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js",
        "keyword.operator.ternary.ts",
        "meta.brace.round.ts",
        "meta.brace.square.ts",
        "punctuation.definition.block.ts",
        "punctuation.definition.binding-pattern.object.ts",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts",
        "keyword.operator.ternary.tsx",
        "meta.brace.round.tsx",
        "meta.brace.square.tsx",
        "punctuation.definition.block.tsx",
        "punctuation.definition.template-expression.begin.tsx",
        "punctuation.definition.template-expression.end.tsx",
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.begin.jsx",
        "punctuation.section.embedded.end.jsx",
        "punctuation.section.embedded.begin.ts",
        "punctuation.section.embedded.end.ts",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx"
      ],
      "settings": {
        "foreground": "#D02E1F"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#ECEEF5"
      }
    },
    {
      "name": "Filename in Find in Files",
      "scope": [
        "entity.name.filename.find-in-files"
      ],
      "settings": {
        "foreground": "#FCFD28"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "meta.function-call.python",
        "new.expr.js"
      ],
      "settings": {
        "foreground": "#888BFF"
      }
    },
    {
      "name": "Inherited class",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#e0cbff",
        "fontStyle": "italic underline"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "name": "JSON String",
      "scope": [
        "meta.structure.dictionary.json",
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#CFCFC2"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#888BFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Library constant",
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#9575ff"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function"
      ],
      "settings": {
        "foreground": "#9575ff"
      }
    },
    {
      "name": "Library variable",
      "scope": [
        "support.other.variable"
      ],
      "settings": {}
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold.markdown"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Emphasis",
      "scope": [
        "markup.italic.markdown"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List Punctuation",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#7c5dfa"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#faf5ff"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#D02E1F"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor.optional.js.jsx"
      ],
      "settings": {
        "foreground": "#79767d"
      }
    },
    {
      "name": "Punctuation Definition",
      "scope": [
        "punctuation.definition.block.js",
        "punctuation.definition.binding-pattern.object.js",
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js"
      ],
      "settings": {
        "foreground": "#79767d"
      }
    },
    {
      "name": "Punctuation Section",
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.begin.jsx",
        "punctuation.section.embedded.end.jsx"
      ],
      "settings": {
        "foreground": "#7c5dfa"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#faf8ff"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#faf8ff"
      }
    },
    {
      "name": "Storage type",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#9575ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#888BFF"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#ED9B00"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag",
        "support.class.component.jsx",
        "support.class.component.js",
        "support.class.component.ts",
        "support.class.component.tsx"
      ],
      "settings": {
        "foreground": "#8B8DFF"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#7c5dfa"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#faf8ff"
      }
    }
  ]
}