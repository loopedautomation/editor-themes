  "tokenColors": [
    {
      "scope": [
        "comment",
        "definition.comment"
      ],
      "settings": {
        "foreground": "#a5a5a5"
      }
    },
    {
      "scope": [
        "constant.character",
        "constant.other",
        "support.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "scope": [
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#7E5DE9"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#5f97f0"
      }
    },
    {
      "scope": [
        "constant.numeric.line-number.find-in-files - match"
      ],
      "settings": {
        "foreground": "#5f97f0"
      }
    },
    {
      "scope": [
        "meta.diff",
        "diff.header",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#030306"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#4A1ED9"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "scope": [
        "keyword.operator.ternary.jsx",
        "keyword.operator.ternary.js",
        "keyword.operator.ternary.ts",
        "keyword.operator.ternary.tsx"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#EE9A00"
      }
    },
    {
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#D02E1E"
      }
    },
    {
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#5f97f0"
      }
    },
    {
      "scope": [
        "markup.heading.setext"
      ],
      "settings": {
        "foreground": "#5f97f0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#5f97f0"
      }
    },
    {
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#389469"
      }
    },
    {
      "scope": [
        "markup.list.numbered.markdown",
        "markup.list.unnumbered.markdown"
      ],
      "settings": {
        "foreground": "#030306"
      }
    },
    {
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "scope": [
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#685EF6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#030306"
      }
    },
    {
      "scope": [
        "punctuation.section",
        "meta.brace",
        "meta.bracket"
      ],
      "settings": {
        "foreground": "#a5a5a5"
      }
    },
    {
      "scope": [
        "meta.brace.round.jsx",
        "meta.brace.square.jsx",
        "meta.brace.round.js",
        "meta.brace.square.js",
        "meta.brace.round.ts",
        "meta.brace.square.ts",
        "meta.brace.round.tsx",
        "meta.brace.square.tsx"
      ],
      "settings": {
        "foreground": "#a5a5a5"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "meta.function.parameter"
      ],
      "settings": {
        "foreground": "#030306"
      }
    },
    {
      "scope": [
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#a5a5a5"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression.begin.jsx",
        "punctuation.definition.template-expression.end.jsx",
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts",
        "punctuation.definition.template-expression.begin.tsx",
        "punctuation.definition.template-expression.end.tsx"
      ],
      "settings": {
        "foreground": "#cc2e3b"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.begin.jsx",
        "punctuation.section.embedded.end.jsx",
        "punctuation.section.embedded.begin.ts",
        "punctuation.section.embedded.end.ts",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx"
      ],
      "settings": {
        "foreground": "#D02E1E"
      }
    },
    {
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#685ef6d5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "scope": [
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#D02E1E"
      }
    },
    {
      "scope": [
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#D02E1E"
      }
    },
    {
      "scope": [
        "variable",
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#030306"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#030306"
      }
    },
    {
      "scope": [
        "token.debug-token"
      ],
      "settings": {
        "foreground": "#D02E1E"
      }
    },
    {
      "scope": [
        "token.error-token"
      ],
      "settings": {
        "foreground": "#D02E1E"
      }
    },
    {
      "scope": [
        "token.info-token"
      ],
      "settings": {
        "foreground": "#8B8DFF"
      }
    },
    {
      "scope": [
        "token.warn-token"
      ],
      "settings": {
        "foreground": "#EE9A00"
      }
    },
    {
      "name": "Boolean values",
      "scope": [
        "constant.language.boolean.true",
        "constant.language.boolean.false"
      ],
      "settings": {
        "foreground": "#7E5DE9"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#5f97f0"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "name": "Filename in Find in Files",
      "scope": [
        "entity.name.filename.find-in-files"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "meta.function-call.python",
        "new.expr.js",
        "support.function"
      ],
      "settings": {
        "foreground": "#030306"
      }
    },
    {
      "name": "Inherited class",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#5f97f0",
        "fontStyle": "italic underline"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#D02E1E"
      }
    },
    {
      "name": "JSON String",
      "scope": [
        "meta.structure.dictionary.json",
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#685EF6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function"
      ],
      "settings": {
        "foreground": "#030306"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold.markdown"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Emphasis",
      "scope": [
        "markup.italic.markdown"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List Punctuation",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#030306"
      }
    },
    {
      "name": "Math and Arithmetic Operators",
      "scope": [
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.increment",
        "keyword.operator.decrement"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "name": "Punctuation Definition",
      "scope": [
        "punctuation.definition.binding-pattern.js",
        "punctuation.definition.binding-pattern.jsx",
        "punctuation.definition.binding-pattern.ts",
        "punctuation.definition.binding-pattern.tsx",
        "punctuation.definition.binding-pattern.binding-pattern.object.js",
        "punctuation.definition.binding-pattern.binding-pattern.object.jsx",
        "punctuation.definition.binding-pattern.binding-pattern.object.ts",
        "punctuation.definition.binding-pattern.binding-pattern.object.tsx"
      ],
      "settings": {
        "foreground": "#a5a5a5"
      }
    },
    {
      "name": "Punctuation Definition",
      "scope": [
        "punctuation.definition.block.js",
        "punctuation.definition.block.jsx",
        "punctuation.definition.block.ts",
        "punctuation.definition.block.tsx"
      ],
      "settings": {
        "foreground": "#a5a5a5"
      }
    },
    {
      "name": "Punctuation Section",
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.begin.jsx",
        "punctuation.section.embedded.end.jsx"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#030306"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#030306"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#030306"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#5f97f0"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag",
        "support.class.component.jsx",
        "support.class.component.js",
        "support.class.component.ts",
        "support.class.component.tsx"
      ],
      "settings": {
        "foreground": "#cc2e3b"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "storage.type.primitive"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    },
    {
      "name": "URLs",
      "scope": [
        "string.other.link",
        "url.*url*",
        "url.*link*",
        "url.*uri*"
      ],
      "settings": {
        "foreground": "#685EF6"
      }
    }
  ]
}