    "notebook.cellInsertionIndicator": "#685EF6",
    "notebook.cellStatusBarItemHoverBackground": "#EEEEEE",
    "notebook.cellToolbarSeparator": "#DEDEDE",
    "notebook.focusedCellBackground": "#DEDEDE",
